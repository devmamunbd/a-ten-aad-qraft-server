{
    "version": 2,
    "builds": [
        {
            "src": "/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods": ["PUT", "PATCH", "GET", "DELETE", "POST", "OPTIONS"]
        }
    ]

}